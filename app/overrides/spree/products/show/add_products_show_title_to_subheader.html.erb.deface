<!-- replace 'h1.product-title' -->
<% content_for :subheader do %> 
  <%= breadcrumbs(@taxon, @product) %>
	<% tempProp = @product_properties.index_by(&:property_id)%>
	<%= @product.to_json %>
  <h1 class="product-title" itemprop="name"><%= tempProp[1].value %> - <%= accurate_title %></h1>
<% end %>